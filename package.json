{
  "name": "@niuee/board",
  "version": "0.0.2",
  "description": "",
  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "rollup": "rollup -c --bundleConfigAsCjs",
    "build": "pnpm run rollup && pnpm run move-package",
    "move-package": "node ./scripts/move-package.mjs",
    "sync-package-version": "node ./scripts/sync-package-version.mjs",
    "doc:setup": "pnpm exec typedoc --options ./typedoc.json",
    "doc:parse-raw": "node ./scripts/parse-raw.mjs",
    "doc:gen-translate-json": "node ./scripts/gen-translation-file.mjs",
    "doc:inject-translation": "node ./scripts/inject-translation-file.mjs",
    "doc:gen-staging-doc": "run(){ if [ $# -eq 0 ]; then echo \"No Language Code provided. Going for us as default\"; pnpm exec typedoc --options ./typedoc-config/staging.typedoc.json --readme ./README.md --entryPoints ./doc-jsons/staging/api.json --out ./docs-staging/en; else echo \"Language Code is: $1\"; pnpm exec typedoc --options ./typedoc-config/staging.typedoc.json --readme ./doc-jsons/staging/$1/README.md --entryPoints ./doc-jsons/staging/$1/api.json --out ./docs-staging/$1; fi;}; run",
    "doc:move2prod": "node ./scripts/doc-transition.mjs",
    "increment-major-version": "cd ./build && pnpm version major",
    "increment-minor-version": "cd ./build && pnpm version minor",
    "increment-patch-version": "cd ./build && pnpm version patch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niuee/board.git"
  },
  "author": "niuee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/niuee/board/issues"
  },
  "homepage": "https://github.com/niuee/board#readme",
  "devDependencies": {
    "@rollup/plugin-html": "^1.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.1",
    "@types/rollup-plugin-generate-package-json": "^3.2.8",
    "css-loader": "^6.8.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "point2point": "^0.0.94",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typedoc": "^0.25.9",
    "typedoc-material-theme": "^1.0.2",
    "typedoc-plugin-mdn-links": "^3.1.17",
    "typescript": "^5.0.3"
  }
}